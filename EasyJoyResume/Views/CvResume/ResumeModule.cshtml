        case "resume_head":
            <div class="headItem moduleItem resume_sort template_css @headShow" data-size="@Resume.resume_headType" id="resume_head">
                <dl>
                    <dt class="clearfix">
                        <a class="jyfont divIconFont" for-key="resume_head">@Resume.iconFontMap.resume_head</a>
                        <span>
                            <div class="item-title module_item_title" data-placeholder="头像" data-textlang="head" contenteditable="false">@Resume.modul_show.resume_head.title</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <!--头像尺寸设置弹框-->
                        <div class="head_size_edit"></div>
                        <div class="head_size_selector">
                            <span class="head_size_title">头像风格</span>
                            <span class="head_size_option" data-size="round"></span>
                            <span class="head_size_option" data-size="square"></span>
                            <span class="head_size_option" data-size="rectangle"></span>
                        </div>
                        <div class="head-con">
                            <a class="img-preview-hover"></a>
                            <div class="img-preview">
                                <img src="@Resume.resume_head" />
                            </div>
                        </div>
                    </dd>
                </dl>
            </div>
            break;
