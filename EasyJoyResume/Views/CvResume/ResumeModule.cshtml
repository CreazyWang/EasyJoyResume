@{
    string item = ViewBag.item;
    EasyJoyResume.Models.ViewModel.resume_base Resume = ViewBag.ResumeBase;
    string headShow = "hidden";
    if (Resume.modul_show.headShow)
    {
        headShow = "";
    }
    switch (item)
    {
        case "resume_head":
            <div class="headItem moduleItem resume_sort template_css @headShow" data-size="@Resume.resume_headType" id="resume_head">
                <dl>
                    <dt class="clearfix">
                        <a class="jyfont divIconFont" for-key="resume_head">@Resume.iconFontMap.resume_head</a>
                        <span>
                            <div class="item-title module_item_title" data-placeholder="头像" data-textlang="head" contenteditable="false">@Resume.modul_show.resume_head.title</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <!--头像尺寸设置弹框-->
                        <div class="head_size_edit"></div>
                        <div class="head_size_selector">
                            <span class="head_size_title">头像风格</span>
                            <span class="head_size_option" data-size="round"></span>
                            <span class="head_size_option" data-size="square"></span>
                            <span class="head_size_option" data-size="rectangle"></span>
                        </div>
                        <div class="head-con">
                            <a class="img-preview-hover"></a>
                            <div class="img-preview">
                                <img src="@Resume.resume_head" />
                            </div>
                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "base_info":
            <div class="infoItem bInfoItem moduleItem resume_sort template_css @ViewBag.isShow" id="base_info">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix">
                        <a class="jyfont divIconFont" for-key="base_info">@Resume.iconFontMap.base_info</a>
                        <span>
                            <div class="resume_lang module_item_title" for-key="info_title" data-placeholder="基本信息" data-placelang="baseMsg" data-textlang="baseMsg" contenteditable="false">@Resume.modul_show.base_info.title</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <div class="info-con">
                            <!-- 渲染时 从工作年限开始没有内容的 div 添加 hidden 类  身高 & 体重 要加小写单位 -->
                            <div class="info-sex info-list moduleItemList @ViewBag.sexShow" id="sex">
                                <a class="jyfont divIconFont" for-key="sex">@Resume.iconFontMap.sex</a>
                                <span>@Resume.resume_base_info.sex</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-age info-list moduleItemList @ViewBag.birthShow" id="birth">
                                <a class="jyfont divIconFont" for-key="birth">@Resume.iconFontMap.birth</a>
                                <span data-placelang="age" data-value="" data-placeholder="年龄">@Resume.resume_base_info.birth</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-nation info-list moduleItemList @ViewBag.nationShow" id="nation">
                                <a class="jyfont divIconFont" for-key="nation">@Resume.iconFontMap.nation</a>
                                <span>@Resume.resume_base_info.nation</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-highedu info-list moduleItemList @ViewBag.educationShow" id="education">
                                <a class="jyfont divIconFont" for-key="education">@Resume.iconFontMap.education</a>
                                <span>@Resume.resume_base_info.education</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-marital info-list moduleItemList @ViewBag.marriageStatusShow" id="marriageStatus">
                                <a class="jyfont divIconFont" for-key="marriageStatus">@Resume.iconFontMap.marriageStatus</a>
                                <span>@Resume.resume_base_info.marriageStatus</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-height info-list moduleItemList @ViewBag.heightShow" id="height">
                                <a class="jyfont divIconFont" for-key="height">@Resume.iconFontMap.height</a>
                                <span>@Resume.resume_base_info.height cm</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-status info-list moduleItemList @ViewBag.politicalStatusShow" id="politicalStatus">
                                <a class="jyfont divIconFont" for-key="politicalStatus">@Resume.iconFontMap.politicalStatus</a>
                                <span>@Resume.resume_base_info.politicalStatus</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-city info-list moduleItemList @ViewBag.cityShow" id="city">
                                <a class="jyfont divIconFont" for-key="city">@Resume.iconFontMap.city</a>
                                <span data-placelang="city" data-value="">@Resume.resume_job_preference.jobCityName</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-work info-list moduleItemList @ViewBag.jobYearShow" id="jobYear">
                                <a class="jyfont divIconFont" for-key="jobYear">@Resume.iconFontMap.jobYear</a>
                                <span data-value="" data-placelang="jobYear" data-placeholder="工作年限">@Resume.resume_base_info.jobYear</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-phone info-list moduleItemList @ViewBag.mobileShow" id="mobile">
                                <a class="jyfont divIconFont" for-key="mobile">@Resume.iconFontMap.mobile</a>
                                <span data-placelang="phone" data-placeholder="联系电话">@Resume.resume_base_info.mobile</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-email info-list moduleItemList @ViewBag.emailShow" id="email">
                                <a class="jyfont divIconFont" for-key="email">@Resume.iconFontMap.email</a>
                                <span data-placelang="email" data-placeholder="电子邮箱">@Resume.resume_base_info.email</span>
                                <a class="delete"></a>
                            </div>
                            <div class="info-weight info-list moduleItemList @ViewBag.weightShow" id="weight">
                                <a class="jyfont divIconFont" for-key="weight">@Resume.iconFontMap.weight</a>
                                <span>@Resume.resume_base_info.weight kg</span>
                                <a class="delete"></a>
                            </div>
                            <!--自定义基本信息-->
                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "base_home":
            <div class="homeItem bInfoItem moduleItem resume_sort template_css @ViewBag.isShow" data-parts="" id="base_home">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix">
                        <a class="jyfont divIconFont" for-key="base_home">@Resume.iconFontMap.base_home</a>
                        <span><div class="resume_lang module_item_title" for-key="base_home" data-placeholder="个人主页" data-placelang="homePage" data-textlang="homePage" contenteditable="false">个人主页</div></span>
                    </dt>
                    <dd class="clearfix">
                        <div class="home-con">
                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "base_social":
            <div class="socialItem bInfoItem moduleItem resume_sort template_css  @ViewBag.isShow" id="base_social">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix">
                        <a class="jyfont divIconFont" for-key="base_social">@Resume.iconFontMap.base_social</a>
                        <span>
                            <div class="resume_lang module_item_title" for-key="base_social" data-placeholder="社交账号" data-placelang="social" data-textlang="social" contenteditable="false">社交账号</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <div class="social-con">
                            <div class="social-wx social-list moduleItemList hidden"><a class="hvr-buzz" href="javascript:;" data-title="微信"></a><span>微信</span></div>
                            <div class="social-qq social-list moduleItemList hidden"><a class="hvr-buzz" href="javascript:;" data-title="QQ"></a><span>QQ</span></div>
                            <div class="social-zh social-list moduleItemList hidden"><a class="hvr-buzz" href="#" title="知乎"></a><span>知乎</span></div>
                            <div class="social-wb social-list moduleItemList hidden"><a class="hvr-buzz" href="#" title="微博"></a><span>微博</span></div>
                            <div class="social-dd social-list moduleItemList hidden"><a class="hvr-buzz" href="javascript:;" data-title="钉钉号"></a><span>钉钉号</span></div>
                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "resume_skill":
            <div class="skillItem baseItem moduleItem resume_sort template_css  @ViewBag.isShow" data-parts="" id="resume_skill">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix @ViewBag.isTitleShow">
                        <a class="jyfont divIconFont" for-key="resume_skill">@Resume.iconFontMap.resume_skill</a>
                        <span>
                            <div class="resume_lang module_item_title" for-key="resume_skill" data-placeholder="技能特长" data-placelang="skill" data-textlang="skill" contenteditable="false">@Resume.modul_show.resume_skill.title</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <div class="skill-con">
                            @if (Resume.resume_skill!=null)
                            {
                                <div class="baseItem-null" style="display: none;">
                                    <div class="baseItem-nullcon">
                                        <i>+</i>
                                        <span data-textlang="addSkills">技能特长</span>
                                    </div>
                                </div>
                                foreach (var skill in Resume.resume_skill)
                                {
                                    <div class="skill-list moduleItemList">
                                        <span class="skill-title item_title"> @skill.name </span>
                                        <span class="skill-slider item_level @skill.masterLevel" data_level="@skill.masterLevel">
                                            <s><i class=" @skill.masterLevel "></i></s>
                                            <span>@skill.masterLevelDesc</span>
                                        </span>
                                        <a class="delete"></a>
                                    </div>
                                }
                            }
                            else
                            {
                                <div class="baseItem-null" style="display: block;">
                                    <div class="baseItem-nullcon">
                                        <i>+</i>
                                        <span data-textlang="addSkills">技能特长</span>
                                    </div>
                                </div>
                            }
                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "resume_hobby":
            <div class="hobbyItem baseItem moduleItem resume_sort template_css @ViewBag.isShow" data-parts="" id="resume_hobby">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix  @ViewBag.isTitleShow">
                        <a class="jyfont divIconFont" for-key="resume_hobby">@Resume.iconFontMap.resume_hobby</a>
                        <span>
                            <div class="resume_lang module_item_title" for-key="resume_hobby" data-placeholder="个人标签" data-placelang="hobby" data-textlang="hobby" contenteditable="false">@Resume.modul_show.resume_hobby.title</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <div class="hobby-con">
                            @if (Resume.resume_hobby != null)
                            {
                                <div class="baseItem-null" style="display: none;">
                                    <div class="baseItem-nullcon">
                                        <i>+</i>
                                        <span data-textlang="addHobby">个人标签</span>
                                    </div>
                                </div>
                                foreach (var hobby in Resume.resume_hobby)
                                {
                                    <div class="hobby-list moduleItemList" id="@hobby.key">
                                        <a class="jyfont divIconFont" for-key="@hobby.key"></a>
                                        <span class="hobby-title item_title"> @hobby.name </span>
                                        <a class="delete"></a>
                                    </div>
                                }
                            }
                            else
                            {
                                <div class="baseItem-null" style="display: block;">
                                    <div class="baseItem-nullcon">
                                        <i>+</i>
                                        <span data-textlang="addHobby">个人标签</span>
                                    </div>
                                </div>
                            }


                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "resume_name":
            <div class="nameItem bInfoItem moduleItem resume_sort template_css @ViewBag.isShow" id="resume_name">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix @ViewBag.isTitleShow">
                        <a class="jyfont divIconFont" for-key="resume_name">@Resume.iconFontMap.resume_name</a>
                        <span><div class="item-title module_item_title" data-placeholder="姓名" contenteditable="false">@Resume.modul_show.resume_name</div></span>
                    </dt>
                    <dd class="clearfix">
                        <div class="name-con">
                            <div class="name resume_lang" for-key="name" data-placeholder="你的名字" data-placelang="name" contenteditable="false">@Resume.resume_base_info.name</div>
                            <div class="word resume_lang" for-key="word" data-placeholder="一句话介绍自己，告诉HR为什么选择你而不是别人" data-placelang="word" contenteditable="false">@Resume.resume_base_info.minSummary</div>
                        </div>
                    </dd>
                </dl>
            </div>
            break;
        case "resume_job_preference":
            <div class="inteItem baseItem moduleItem resume_sort template_css  @ViewBag.isShow" data-parts="" id="resume_job_preference">
                <div class="baseItem-toolbar">
                    <span class="edit"></span>
                    <span class="handle ui-sortable-handle"></span>
                    <span class="add save_opt"></span>
                    <span class="change_style"></span>
                    <span class="set">
                        <div class="set-con">
                            <div class="inner clearfix">
                                <div class="set-list hiddenTitle save_opt"><i>隐藏栏目标题</i><s></s></div>
                                <div class="set-list hiddenTime save_opt"><i>隐藏时间模块</i><s></s></div>
                                <div class="set-list hiddenText save_opt"><i>隐藏描述模块</i><s></s></div>
                                <!--<div class="set-list changeTitle save_opt"><i>修改栏目标题</i><a class="change">修改</a></div>-->
                                <div class="set-list recovery_style save_opt"><i>恢复初始样式</i><a class="recovery ">恢复</a></div>
                                <div class="set-list"><i class="noChange">删除当前模块</i><a class="delete">删除</a></div>
                            </div>
                        </div>
                    </span>
                </div>
                <dl>
                    <dt class="clearfix @ViewBag.isTitleShow">
                        <a class="jyfont divIconFont" for-key="resume_job_preference">@Resume.iconFontMap.resume_job_preference</a>
                        <span>
                            <div class="resume_lang module_item_title" for-key="resume_job_preference" data-placeholder="求职意向" data-placelang="job" data-textlang="job" contenteditable="false">@Resume.modul_show.resume_job_preference.title</div>
                        </span>
                    </dt>
                    <dd class="clearfix">
                        <div class="inte-con">
                            @{
                                if (String.IsNullOrEmpty(Resume.resume_job_preference.jobFunction + Resume.resume_job_preference.jobCity + Resume.resume_job_preference.jobMaxSalary + Resume.resume_job_preference.jobTime + Resume.resume_job_preference.jobMinSalary))
                                {
                                    <div class="inte-job inte-list moduleItemList" id="jobFunction">
                                        <a class="jyfont divIconFont" for-key="jobFunction">@Resume.iconFontMap.jobFunction</a>
                                        <span data-placeholder="意向岗位" data-placelang="jobFun"></span>
                                        <a href="javascript:;" class="delete"></a>
                                    </div>
                                    <div class="inte-type inte-list moduleItemList hidden" id="jobType">
                                        <a class="jyfont divIconFont" for-key="jobType">@Resume.iconFontMap.jobType</a>
                                        <span data-value="" data-placeholder="职业类型" data-placelang="jobType"></span>
                                        <a href="javascript:;" class="delete"></a>
                                    </div>
                                    <div class="inte-city inte-list moduleItemList" id="jobCity">
                                        <a class="jyfont divIconFont" for-key="jobCity">@Resume.iconFontMap.jobCity</a>
                                        <span data-placeholder="意向城市" data-value="" data-placelang="jobCity"></span>
                                        <a href="javascript:;" class="delete"></a>
                                    </div>
                                    <div class="inte-price inte-list moduleItemList" id="jobSalary">
                                        <a class="jyfont divIconFont" for-key="jobSalary">@Resume.iconFontMap.jobSalary</a>
                                        <span data-placeholder="薪资要求" data-placelang="jobSalary"></span>
                                        <a href="javascript:;" class="delete job"></a>
                                    </div>
                                    <div class="inte-time inte-list moduleItemList" id="jobTime">
                                        <a class="jyfont divIconFont" for-key="jobTime">@Resume.iconFontMap.jobTime</a>
                                        <span data-value="" data-placeholder="入职时间" data-placelang="jobTime"></span>
                                        <a href="javascript:;" class="delete"></a>
                                    </div>
                                }
                                else
                                {
                                    if (String.IsNullOrEmpty(Resume.resume_job_preference.jobFunction))
                                    {
                                        <div class="inte-job inte-list moduleItemList hidden" id="jobFunction">
                                            <a class="jyfont divIconFont" for-key="jobFunction">@Resume.iconFontMap.jobFunction</a>
                                            <span data-placeholder="意向岗位" data-placelang="jobFun">@Resume.resume_job_preference.jobFunction</span>
                                            <a href="javascript:;" class="delete"></a>
                                        </div>
                                    }
